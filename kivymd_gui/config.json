{
    "app": {
        "title": "Encrypted Backup Server",
        "version": "2.0.0",
        "theme": "Dark",
        "primary_color": "Blue",
        "material_style": "M3",
        "window": {
            "width": 1400,
            "height": 900,
            "min_width": 1000,
            "min_height": 700
        },
        "expressive_theming": {
            "enable_dynamic": false,
            "seed_color": "#1976D2",
            "harmony": "analogous",
            "expressive_style": "expressive",
            "available_harmonies": [
                "monochromatic",
                "analogous",
                "complementary",
                "triadic",
                "split_complementary"
            ],
            "available_styles": [
                "baseline",
                "expressive"
            ]
        }
    },
    "server": {
        "port": 1256,
        "host": "0.0.0.0",
        "storage_dir": "received_files",
        "max_clients": 50,
        "session_timeout": 10,
        "maintenance_interval": 60
    },
    "ui": {
        "animations": true,
        "show_tooltips": true,
        "auto_refresh_interval": 1.0,
        "log_max_entries": 100,
        "table_rows_per_page": 15
    },
    "charts": {
        "enabled": true,
        "update_interval": 2.0,
        "data_retention_minutes": 60,
        "colors": {
            "cpu": "#1976D2",
            "memory": "#7C4DFF",
            "network": "#00897B",
            "primary": "#1976D2",
            "secondary": "#00897B",
            "tertiary": "#7C4DFF"
        }
    },
    "monitoring": {
        "performance_tracking": true,
        "file_system_monitoring": true,
        "log_monitoring": true,
        "auto_cleanup": {
            "enabled": true,
            "max_log_age_days": 30,
            "max_cache_size_mb": 100
        }
    },
    "security": {
        "enable_https": false,
        "cert_file": "",
        "key_file": "",
        "require_auth": false,
        "session_timeout_minutes": 30
    },
    "advanced": {
        "developer_mode": false,
        "debug_logging": false,
        "experimental_features": false,
        "backup_config": true,
        "config_version": "2.0.0"
    }
}