{
    "python.defaultInterpreterPath": "./flet_venv/Scripts/python.exe",
    "python.terminal.activateEnvironment": true,
    "python.analysis.extraPaths": [
        "./",
        "./FletV2",
        "./Shared",
        "./python_server",
        "./api_server"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.reportMissingImports": "warning",
    "python.analysis.reportMissingTypeStubs": "none",
    "python.analysis.reportUnknownParameterType": "none",
    "python.analysis.reportUnknownArgumentType": "none",
    "python.analysis.reportUnknownMemberType": "none",
    "python.analysis.reportGeneralTypeIssues": "none",
    "python.analysis.reportOptionalCall": "none",
    "python.analysis.reportOptionalMemberAccess": "none",
    "python.analysis.autoSearchPaths": true,
    "python.analysis.useLibraryCodeForTypes": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.stubPath": "./stubs",
    "python.analysis.typeshedPaths": ["./stubs"],
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.analysis.ignore": [
        "**/node_modules/**",
        "**/build/**",
        "**/dist/**",
        "**/.git/**"
    ],
    "python.analysis.include": [
        "./FletV2/**/*.py",
        "./Shared/**/*.py",
        "./python_server/**/*.py",
        "./api_server/**/*.py"
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/node_modules": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "github.copilot.chat.editor.temporalContext.enabled": true,
    "github.copilot.chat.agent.thinkingTool": true,
    "github.copilot.chat.codesearch.enabled": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.chat.newWorkspace.useContext7": true,
    "github.copilot.chat.generateTests.codeLens": true,
    "github.copilot.chat.edits.temporalContext.enabled": true,
    "github.copilot.advanced": {
        "useLanguageServer": true,
        "maxCompletions": 5
    },
    "python-envs.pythonProjects": []
}