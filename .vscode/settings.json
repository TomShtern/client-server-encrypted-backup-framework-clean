{
    "python.defaultInterpreterPath": "./kivy_venv_new/Scripts/python.exe",
    "python.terminal.activateEnvironment": true,
    "python.analysis.extraPaths": [
        "./",
        "./kivymd_gui",
        "./Shared",
        "./python_server",
        "./api_server"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.reportMissingImports": "warning",
    "python.analysis.reportMissingTypeStubs": "none",
    "python.analysis.reportUnknownParameterType": "none",
    "python.analysis.reportUnknownArgumentType": "none",
    "python.analysis.reportUnknownMemberType": "none",
    "python.analysis.reportGeneralTypeIssues": "none",
    "python.analysis.reportOptionalCall": "none",
    "python.analysis.reportOptionalMemberAccess": "none",
    "python.analysis.autoSearchPaths": true,
    "python.analysis.useLibraryCodeForTypes": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.stubPath": "./stubs",
    "python.analysis.typeshedPaths": ["./stubs"],
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": false,
    "python.analysis.ignore": [
        "**/node_modules/**",
        "**/build/**",
        "**/dist/**",
        "**/.git/**"
    ],
    "python.analysis.include": [
        "./kivymd_gui/**/*.py",
        "./Shared/**/*.py",
        "./python_server/**/*.py",
        "./api_server/**/*.py"
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/node_modules": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    }
}