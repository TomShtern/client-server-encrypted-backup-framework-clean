      "warning": 80.0,
      "critical": 95.0,
      "enabled": true
    },
    "disk": {
      "warning": 85.0,
      "critical": 95.0,
      "enabled": true
    },
    "network": {
      "warning": 50.0,
      "critical": 100.0,
      "enabled": true
    }
  }
}